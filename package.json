{
    "name": "opensgf",
    "version": "1.0.0",
    "description": "A starter template for the TEA stack.",
    "main": "index.js",
    "scripts": {
        "build": "npm-run-all -s clean build:webpack build:eleventy",
        "build:eleventy": "cross-env NODE_ENV=production eleventy",
        "build:webpack": "cross-env NODE_ENV=production webpack",
        "clean": "rimraf dist",
        "start": "npm-run-all -s clean -p start:webpack start:eleventy",
        "start:eleventy": "eleventy --serve --quiet",
        "start:webpack": "webpack --watch"
    },
    "keywords": [
        "eleventy",
        "tailwind",
        "alpine"
    ],
    "dependencies": {
        "alpinejs": "^2.8.0"
    },
    "devDependencies": {
        "@11ty/eleventy": "^0.11.0",
        "@tailwindcss/forms": "^0.2.1",
        "autoprefixer": "^10.1.0",
        "cross-env": "^7.0.2",
        "css-loader": "^5.0.1",
        "cssnano": "^4.1.10",
        "html-minifier": "^4.0.0",
        "mini-css-extract-plugin": "^1.3.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.2.1",
        "postcss-import": "^14.0.0",
        "postcss-loader": "^4.1.0",
        "rimraf": "^3.0.2",
        "sass": "^1.30.0",
        "sass-loader": "^10.1.0",
        "style-loader": "^2.0.0",
        "tailwindcss": "^2.0.2",
        "tailwindcss-debug-screens": "^2.0.0",
        "webpack": "^5.11.0",
        "webpack-cli": "^4.2.0",
        "webpack-manifest-plugin": "^3.0.0"
    }
}
